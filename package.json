{
  "name": "elm-hash",
  "version": "1.0.0",
  "description": "Turn elm objects into comparable hash values",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "run-s test:verify-examples test:unit test:docs ",
    "test:verify-examples": "npx elm-verify-examples",
    "test:unit": "npx elm-test",
    "test:docs": "npx elm-doc-preview --output /dev/null",
    "dev:docs": "npx elm-doc-preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Evelios/elm-hash.git"
  },
  "author": "Thomas Waters",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Evelios/elm-hash/issues"
  },
  "homepage": "https://github.com/Evelios/elm-hash#readme",
  "dependencies": {
    "elm": "^0.19.1-3",
    "elm-test": "^0.19.1-revision4",
    "npm-run-all": "^4.1.5"
  }
}
